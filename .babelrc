{
  "presets": ["react"],
  "env": {
    "cjs": {
      "presets": [
        [
          "env",
          {
            "loose": true
          }
        ]
      ],
      "plugins": ["transform-class-properties"]
    },
    "es": {
      "presets": [
        [
          "env",
          {
            "loose": true,
            "modules": false
          }
        ]
      ],
      "plugins": ["transform-class-properties"]
    }
  }
}
