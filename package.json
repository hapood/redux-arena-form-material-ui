{
  "name": "redux-arena-form-material-ui",
  "version": "0.0.3",
  "description":
    "A set of material-ui high order components used in redux-arena-form-material-ui",
  "scripts": {
    "build:cjs":
      "cross-env BABEL_ENV=cjs babel ./src --out-dir ./build/lib --ignore spec.js",
    "build:es":
      "cross-env BABEL_ENV=es babel ./src --out-dir ./build/es --ignore spec.js",
    "build": "run-p build:**",
    "build:copy-files":
      "cross-env BABEL_ENV=cjs babel-node ./scripts/copy-files.js",
    "clean:build": "rimraf build",
    "prebuild": "npm run clean:build",
    "postpublish": "npm run build && cd build && npm publish",
    "test": "karma start test/karma.conf.js",
    "test-travis":
      "karma start test/karma.conf.js --browsers Firefox --single-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hapood/redux-arena-form-material-ui.git"
  },
  "keywords": [
    "react",
    "redux-arena",
    "redux-arena-form-material-ui",
    "material-ui"
  ],
  "author": "Hapood Wang",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/hapood/redux-arena-form-material-ui/issues"
  },
  "homepage": "https://github.com/hapood/redux-arena-form-material-ui#readme",
  "peerDependencies": {
    "redux-arena-form": "<=0.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "cross-env": "^5.0.5",
    "enzyme": "^2.9.1",
    "fs-extra": "^4.0.1",
    "html-webpack-plugin": "^2.30.1",
    "istanbul-instrumenter-loader": "^3.0.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-verbose-reporter": "^0.0.6",
    "karma-webpack": "^2.0.4",
    "loglevel": "^1.4.1",
    "mocha": "^3.5.0",
    "npm-run-all": "^4.1.1",
    "react": "^15.6.1",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "redux-arena": "^0.6.3",
    "redux-arena-form": "^0.0.1",
    "rimraf": "^2.6.1",
    "sinon": "^3.2.1",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.7.1"
  }
}
